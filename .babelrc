{
  "presets": [
    "next/babel"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@components": "./src/components/",
          "@pages": "./src/pages/",
          "@layout": "./src/layout",
          "@model": "./src/models",
          "@store": "./src/store",
          "@service": "./src/service"
        }
      }
    ]
  ]
}